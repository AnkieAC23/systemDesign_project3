generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model outfits {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  userId    String
  rating    Int?
  notes     String?
  photoURL  String
  date      DateTime
  title     String?
  brands    Json?
  occasion  String?
}
